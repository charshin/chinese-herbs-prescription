{
  "extends": ["next/core-web-vitals", "plugin:storybook/recommended", "prettier"],
  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "groups": ["builtin", "external", "internal", "parent", "sibling", "index"]
      }
    ],
    "no-console": "error"
  }
}
